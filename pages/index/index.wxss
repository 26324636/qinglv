/* 保持大小不变的小圆圈  */

.dot {
  position: absolute;
  width: 180rpx;
  height: 180rpx;
  margin-left: -90rpx;
  left: 50%;
  top: 330rpx;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border: 2px solid #fff;
  border-radius: 50%;
  z-index: 2;
  background-image: linear-gradient(135deg, #fd6e6a 10%, #ffc600 100%);
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  line-height: 180rpx;
}

/* 产生动画（向外扩散变大）的圆圈  */

.pulse {
  position: absolute;
  width: 240rpx;
  height: 240rpx;
  margin-left: -130rpx;
  left: 50%;
  top: 290rpx;
  border: 6px solid #ff8c69;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  z-index: 1;
  opacity: 0;
  -webkit-animation: warn 2s ease-out;
  -moz-animation: warn 2s ease-out;
  animation: warn 2s ease-out;
  -webkit-animation-iteration-count: infinite;
  -moz-animation-iteration-count: infinite;
  animation-iteration-count: infinite;
  box-shadow: 1px 1px 30px red;
}

