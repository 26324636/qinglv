.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-jia:before {
  content: "\e62d";
}

.icon-dangao:before {
  content: "\e612";
}

.icon-aixin:before {
  content: "\e600";
}

page {
  background: linear-gradient(to bottom, #ff82ab, #ffb5c5);
    background-repeat:no-repeat;
  background-attachment: fixed;
}

.markday {
  padding-bottom: 20rpx;
}

.list {
  width: 90%;
  margin-left: 5%;
  background: #fff;
  border-radius: 20rpx;
  margin-top: 20rpx;
  padding: 50rpx 0;
}

.content {
  display: flex;
  height: 50rpx;
  position: relative;
}

.icon-dangao {
  margin-left: 5%;
  font-size: 38rpx;
  line-height: 50rpx;
  color: #ff82ab;
}

.icon-aixin {
  margin-left: 5%;
  font-size: 25rpx;
  line-height: 50rpx;
  color: #ff82ab;
}

.title {
  font-size: 30rpx;
  margin-left: 20rpx;
  line-height: 50rpx;
}

.icon-jia {
  position: absolute;
  right: 5%;
  font-size: 50rpx;
  color: #e27f94;
  /* top: -10rpx; */
}

.list-text {
  height: 60rpx;
  margin-left: 20rpx;
  margin-top: -5rpx;
}

.list-text-title {
  height: 30rpx;
  font-size: 30rpx;
}

.list-text-time {
  height: 20px;
  margin-top: 10rpx;
  font-size: 20rpx;
}

.num {
  position: absolute;
  right: 10%;
  color: #e27f94;
    font-size: 50rpx;
      margin-top: -5rpx;
}

.day {
  position: absolute;
  right: 5%;
  font-size: 30rpx;
  line-height: 60rpx;
}
