.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-yumao:before {
  content: "\e634";
}

page {
  background-image: linear-gradient(135deg, #fef5f6 1%, #fef5f6 100%);
}

.note image {
  width: 100%;
  height: 250rpx;
}

.noteList {
  width: 90%;
  margin-left: 5%;
  padding: 25rpx 0;
  color: #CD4C69;
  border-bottom: 1rpx solid #ddd;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 30rpx;
}

.noteList-time {
  margin-top: 15rpx;
  font-size: 25rpx;
}

.toAdd {
  position: fixed;
  right: 50rpx;
  border-radius: 50%;
  bottom: 170rpx;
  width: 100rpx;
  height: 100rpx;
  background: #ef696c;
  text-align: center;
}

.icon-yumao {
  color: white;
  font-size: 45rpx;
  line-height: 100rpx;
}
